      <template>http://dev.eclipse.org/mhonarc/lists/${1}</template>
      <name>Browse Archive</name>
    </org.eclipse.skalli.ext.mapping.mail.MailingListMappingConfig>
  </mailingListMappings>
</org.eclipse.skalli.ext.mapping.mail.MailingListMappingsConfig>