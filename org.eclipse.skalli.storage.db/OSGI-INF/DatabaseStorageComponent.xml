<scr:component xmlns:scr="http://www.osgi.org/xmlns/scr/v1.1.0" immediate="true" name="org.eclipse.skalli.storage.db.persistencedb">
  <implementation class="org.eclipse.skalli.storage.db.PersistenceDB"/>
  <service>
    <provide interface="org.eclipse.skalli.services.persistence.StorageService"/>
  </service>
  <property name="storageService.type" type="String" value="db"/>
  <reference
    interface="org.eclipse.skalli.storage.db.StorageEntityManagerService"
    name="EntityManagerService"
    policy="dynamic"
    cardinality="1..1"
    bind="bindEntityManagerService"
    unbind="unbindEntityManagerService" />
</scr:component>
